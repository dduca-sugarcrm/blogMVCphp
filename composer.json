{
  "name": "MVCblog/MVCblog",
  "description": "Jast simple MVC blog",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Duka Dmitry",
      "email": "mc_dir@mail.ru"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "prepend-autoloader": true,
    "autoloader-suffix": "mvc",
    "notify-on-install": true,
    "discard-changes": true,
    "preferred-install": "dist",
    "minimum-stability": "dev",
    "use-include-path": false
  },
  "require": {
    "php": ">=5.3.0",
    "leafo/lessphp": "*",
    "twitter/bootstrap": "3.3.5",
    "twig/twig": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.1.4"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "version": "dev-master",
        "name": "twitter/bootstrap",
        "source": {
          "url": "https://github.com/twbs/bootstrap.git",
          "type": "git",
          "reference": "v3.3.5"
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Core\\": "app"
    },
    "classmap": [
      "app/controllers",
      "app/models",
      "app/views"
    ]
  }
}
